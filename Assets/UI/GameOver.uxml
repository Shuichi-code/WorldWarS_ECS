            <ui:Button text="Exit Game" display-tooltip-when-elided="true" name="exitBtn" style="width: 30%; height: 30%; background-color: rgb(192, 57, 43); -unity-font-definition: resource(&apos;Fonts &amp; Materials/LiberationSans SDF&apos;);" />
